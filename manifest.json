{
    "name": "CoviHelp - Slots finder",
    "description": "Automatically searches for covid vaccination slots and notifies you when a matching slot is found",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "48": "icon48.png"
    },
    "action": {
        "default_icon": "icon48.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "notifications",
        "alarms"
    ],
    "background": {
        "service_worker": "service.js"
    }
}